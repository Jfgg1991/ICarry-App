        <td>
            @Html.DisplayFor(modelItem => item.numeroChasis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tecnomecanica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.permisoCarga)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.capacidadCarga)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descripcionVehiculo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Placa }) |
