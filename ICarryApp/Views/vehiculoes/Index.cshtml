        <th>
            @Html.DisplayNameFor(model => model.tecnomecanica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.permisoCarga)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.capacidadCarga)
        </th>
